<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="root" prefHeight="458.0" prefWidth="372.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LoginPageController">
   <children>
      <Label layoutX="127.0" layoutY="7.0" prefHeight="56.0" prefWidth="207.0" text="Login to To Do List" textFill="#ff0505">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="56.0" layoutY="99.0" prefHeight="4.0" prefWidth="130.0" text="User Name">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label layoutX="56.0" layoutY="181.0" prefHeight="8.0" prefWidth="81.0" text="Password">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <TextField fx:id="txtUsername" layoutX="56.0" layoutY="129.0" prefHeight="26.0" prefWidth="250.0" />
      <PasswordField fx:id="txtPassword" layoutX="56.0" layoutY="209.0" onAction="#txtPasswordOnAction" prefHeight="26.0" prefWidth="250.0" />
      <Button layoutX="59.0" layoutY="273.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="38.0" prefWidth="250.0" style="-fx-background-color: blue;" text="Login" textFill="#fdfdfd">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="127.0" layoutY="326.0" onMouseClicked="#lblCreateNewAccountOnMouseClicked" prefHeight="26.0" prefWidth="207.0" text="Create New Account" textFill="#590cfa" underline="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Label>
      <Label layoutX="121.0" layoutY="436.0" text="Designed by : Erandi Ranasinghe">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
